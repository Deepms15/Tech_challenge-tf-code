{
    "Version": "2012-10-17",
    "Id": "s3-bucket-ssl-requests-only rule",
    "Statement": [
        {
            "Sid": "AllowSSLRequestsOnly",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::tech-challenge-share",
                "arn:aws:s3:::tech-challenge-share/*"
            ],
            "Condition": {
                "Bool": {
                    "aws:SecureTransport": "false"
                }
            }
        }
    ]
}